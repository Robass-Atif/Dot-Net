
@{
    ViewData["Title"] = "ConfirmDeleteFeedback";
    Layout = "~/Views/Shared/_Dashboard.cshtml";
}
@model Feedback
<h1>ConfirmDeleteFeedback</h1>
<div class="container mt-5">
	<div class="card shadow-sm">
		<div class="card-body">
			<h5 class="card-title
">Are you sure you want to delete this feedback?</h5>
			<p class="card-text">Feedback ID: @Model.feedback_id</p>
			<p class="card-text">Customer ID: @Model.username</p>
			<p class="card-text">Feedback: @Model.user_message</p>

			<a href="@Url.Action("FeedBackAdmin", "Admin")" class="btn btn-secondary">
					Cancel</a>
			<a href="@Url.Action("DeleteFeedback", "Admin", new { id = Model.feedback_id })" class="btn btn-danger" >
				Delete

					</a>
			
		</div>
	</div>

